<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context="Frag_adm_menu_res_aula.ReservarAulasFragment">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:id="@+id/textView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView24"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/textView24"
        android:layout_below="@+id/textView33"
        android:layout_marginTop="12dp"
        android:padding="3dp"
        android:text="Prof:"
        android:textAlignment="textEnd"
        android:textColor="@android:color/background_light"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="40dp" />

    <FrameLayout
        android:id="@+id/frameLayout6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView38"
        android:background="@android:color/background_light"
        android:layout_alignRight="@+id/frameLayout6"
        android:layout_alignEnd="@+id/frameLayout6"
        android:layout_alignBottom="@+id/textView38"
        android:layout_toEndOf="@+id/textView33"
        android:layout_toRightOf="@+id/textView33">

        <Spinner
            android:id="@+id/sp_listarProfReservarAula"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </FrameLayout>

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView24"
        android:layout_alignStart="@+id/textView24"
        android:layout_below="@+id/frameLayout6"
        android:layout_marginTop="15dp"
        android:padding="3dp"
        android:text="Discip:"
        android:textAlignment="textEnd"
        android:textColor="@android:color/background_light"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="40dp" />

    <FrameLayout
        android:id="@+id/frameLayout6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/background_light"
        android:layout_alignBottom="@+id/textView33"
        android:layout_alignRight="@+id/frameLayout6"
        android:layout_alignEnd="@+id/frameLayout6"
        android:layout_alignTop="@+id/textView33"
        android:layout_toRightOf="@+id/textView33"
        android:layout_toEndOf="@+id/textView33">

        <Spinner
            android:id="@+id/sp_listarDiscipReservarAula"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </FrameLayout>

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="19dp"
        android:padding="3dp"
        android:text="Bloco:"
        android:textAlignment="textEnd"
        android:textColor="@android:color/background_light"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="40dp" />

    <FrameLayout
        android:id="@+id/frameLayout5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/background_light"
        android:layout_alignTop="@+id/textView24"
        android:layout_toEndOf="@+id/textView33"
        android:layout_toRightOf="@+id/textView33"
        android:layout_alignBottom="@+id/textView24">

        <Spinner
            android:id="@+id/sp_listarBlocosReservarAula"
            android:layout_width="76dp"
            android:layout_height="match_parent" />
    </FrameLayout>

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/frameLayout5"
        android:layout_toEndOf="@+id/frameLayout5"
        android:layout_toRightOf="@+id/frameLayout5"
        android:padding="3dp"
        android:text="Sala:"
        android:textColor="@android:color/background_light"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="40dp" />

    <FrameLayout
        android:id="@+id/frameLayout6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView28"
        android:layout_toEndOf="@+id/textView28"
        android:layout_toRightOf="@+id/textView28"
        android:background="@android:color/background_light"
        android:layout_alignBottom="@+id/textView28">

        <Spinner
            android:id="@+id/sp_listarSalasReservarAula"
            android:layout_width="81dp"
            android:layout_height="match_parent" />
    </FrameLayout>

    <Button
        android:id="@+id/btn_ReservarAula"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Agendar"
        android:layout_below="@+id/textView35"
        android:layout_alignLeft="@+id/textView33"
        android:layout_alignStart="@+id/textView33"
        android:layout_marginTop="12dp" />

    <TextView
        android:id="@+id/textView43"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView38"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/txt_dataEscolhida"
        android:layout_toStartOf="@+id/txt_dataEscolhida"
        android:padding="3dp"
        android:text="Data:"
        android:textColor="@android:color/background_light"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="40dp" />

    <TextView
        android:id="@+id/txt_dataEscolhida"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView43"
        android:layout_alignBottom="@+id/textView43"
        android:layout_toEndOf="@+id/textView38"
        android:layout_toRightOf="@+id/textView38"
        android:padding="3dp"
        android:textColor="@android:color/background_light"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="40dp" />

    <TextView
        android:id="@+id/textView45"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="3dp"
        android:text="Horarios"
        android:textColor="@android:color/background_light"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="40dp"
        android:layout_below="@+id/btn_date"
        android:layout_toLeftOf="@+id/txt_dataEscolhida"
        android:layout_toStartOf="@+id/txt_dataEscolhida" />

    <Button
        android:id="@+id/btn_verListaAgendamentos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/btn_ReservarAula"
        android:layout_toEndOf="@+id/btn_ReservarAula"
        android:layout_toRightOf="@+id/btn_ReservarAula"
        android:text="Ver agendamentos" />

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/background_light"
        android:orientation="horizontal"
        android:layout_alignBottom="@+id/textView35"
        android:layout_toRightOf="@+id/txt_dataEscolhida"
        android:layout_alignRight="@+id/btn_date"
        android:layout_alignEnd="@+id/btn_date">

        <Spinner
            android:id="@+id/sp_mostrarHorariosDisp"
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:layout_above="@+id/btn_verListaAgendamentos"
            android:layout_toEndOf="@+id/linearLayout7"
            android:layout_toRightOf="@+id/linearLayout7" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView45"
        android:text="Disp:"
        android:textAlignment="textEnd"
        android:textColor="@android:color/background_light"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_toStartOf="@+id/linearLayout7"
        android:layout_alignRight="@+id/textView45"
        android:layout_alignEnd="@+id/textView45" />

    <Button
        android:id="@+id/btn_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Escolher Data"
        android:layout_alignBaseline="@+id/txt_dataEscolhida"
        android:layout_alignBottom="@+id/txt_dataEscolhida"
        android:layout_alignRight="@+id/frameLayout6"
        android:layout_alignEnd="@+id/frameLayout6" />

</RelativeLayout>
